<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%msg%n</pattern>
		</encoder>
	</appender>

	<logger name='eg.tedyoung' level='INFO'/>
	
	<root level="WARN">
		<appender-ref ref="STDOUT"/>
	</root>
</configuration>